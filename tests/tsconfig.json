    "resolveJsonModule": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
